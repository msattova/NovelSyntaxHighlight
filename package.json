{
  "name": "novel-syntax-highlight",
  "displayName": "Japanese Novel SyntaxHighlight",
  "description": "Provides Japanese novels syntax highlight for Visual Studio Code",
  "publisher": "1e2c4eef-7941-6152-8e08-66c5f73a473e",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "plaintext",
        "aliases": [
          "Plain Text",
          "plaintext"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "plaintext",
        "scopeName": "source.txt",
        "path": "./syntaxes/plaintext.tmLanguage.json"
      }
    ]
  }
}
